<div id="tambah-paket-modal" class="modal" tabindex="-1" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-body p-0">
                <form class="validate-form m-5 pt-5">
                    <div class="input-form">
                        <label for="ps" class="form-label w-full flex flex-col sm:flex-row">
                            Tipe PS</label>
                        <input id="ps" type="text" name="ps" class="form-control"
                            placeholder="Masukkan Tipe PS..." minlength="2" required>
                    </div>
                    <div class="input-form mt-3">
                        <label for="namapaket" class="form-label w-full flex flex-col sm:flex-row">
                            Nama Paket </label>
                        <input id="namapaket" type="text" name="namapaket" class="form-control"
                            placeholder="Masukkan Nama Paket..." required>
                    </div>
                    <div class="input-form mt-3">
                        <label for="biaya" class="form-label w-full flex flex-col sm:flex-row">
                            Biaya Perjam </label>
                        <input id="biaya" type="number" name="biaya" class="form-control"
                            placeholder="Masukkan Nominal Biaya..." minlength="6" required>
                    </div>
                    <div class="input-form mt-3">
                        <label for="durasi" class="form-label w-full flex flex-col sm:flex-row">
                            Durasi </label>
                        <input id="durasi" type="number" name="durasi" class="form-control"
                            placeholder="Masukkan Durasi..." required>
                    </div>
                    <div class="input-form mt-3">
                        <label for="durasi" class="form-label">Status</label>
                        <select data-placeholder="Pilih status..." class="tom-select w-full" id="durasi"
                            name="durasi" multiple>
                            <option value="active">Active</option>
                            <option value="pending">Pending</option>
                        </select>
                    </div>
                </form>
                <div class="px-5 pb-8 text-center">
                    <button type="button" data-tw-dismiss="modal"
                        class="btn btn-outline-secondary w-24 mr-1">Batal</button>
                    <button type="button" class="btn btn-primary w-24">Simpan</button>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- END: Tambah Paket Modal -->
